{
  "name": "api-1",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "env-cmd -f .env nodemon --exec babel-node ./src/bin/server.js",
    "dev": "env-cmd -f .env babel-node ./src/bin/server.js",
    "clean": "rm -rf dist",
    "build": "npm run clean && babel ./src --out-dir dist --copy-files",
    "prod": "node ./dist/bin/server.js",
    "lint": "eslint  **/*.js --color",
    "format": "prettier --write .",
    "fmt": "npm run format && npm run lint",
    "isready": "npm run format && npm run lint && npm run build",
    "migrate": "env-cmd -f .env npx sequelize-cli db:migrate",
    "migrate:reset": "env-cmd -f .env npx sequelize-cli db:migrate:undo:all && npm run migrate"
  },
  "dependencies": {
    "babel-plugin-module-resolver": "^4.1.0",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.0",
    "cookie-parser": "~1.4.4",
    "core-js": "^3.18.3",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "env-cmd": "^10.1.0",
    "eslint-import-resolver-node": "^0.3.6",
    "express": "~4.17.1",
    "http-errors": "~1.6.3",
    "jsonwebtoken": "^8.5.1",
    "log-node": "^8.0.2",
    "pg": "^8.7.1",
    "pg-hstore": "^2.3.4",
    "ramda": "^0.26.1",
    "regenerator-runtime": "^0.13.7",
    "require-all": "^3.0.0",
    "sequelize": "^6.8.0",
    "swagger-jsdoc": "^5.0.1",
    "swagger-ui-express": "^4.1.6"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/eslint-parser": "^7.15.8",
    "@babel/node": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "babel-register": "^6.26.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-react": "^7.26.1",
    "nodemon": "^2.0.14",
    "prettier": "^2.4.1",
    "prettier-eslint": "^8.2.2",
    "prettier-eslint-cli": "^4.2.1",
    "react": "^17.0.2",
    "sequelize-cli": "^6.2.0"
  },
  "engines": {
    "node": "14.X"
  }
}
